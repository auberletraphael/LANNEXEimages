      procédure chargeINI()
      //Lecture du fichier INI avec les identifiants FTP
      sFichierINI est une chaine = fRepEnCours() + "\PARAM.INI"
      
      SAI_adresse = INILit("LANNEXEFTP", "ADDR", "Vide", sFichierINI)
      SAI_port = INILit("LANNEXEFTP", "PORT", "Vide", sFichierINI)
      SAI_Utilisateur = INILit("LANNEXEFTP", "USER", "Vide", sFichierINI)
      SAI_password = Décrypte(INILit("LANNEXEFTP", "PASSWORD", "Vide", sFichierINI),"sicalait2020",compresseChaîneCourte,Vrai)
      
      
      
     type : 458752
   -
     name : ecritINI
     procedure_id : 2234646581904068726
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // ecritINI ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      procédure ecritINI()
      sFichierINI		est une chaine	= fRepEnCours() + "\PARAM.INI"
      
      sChaîneCryptée	est une chaîne
      sChaîneCryptée = Crypte(SAI_password, "sicalait2020", CompresseChaîneCourte, Vrai)
      
      
      INIEcrit("LANNEXEFTP", "ADDR", SAI_adresse, sFichierINI)
      INIEcrit("LANNEXEFTP", "USER", SAI_Utilisateur, sFichierINI)
      INIEcrit("LANNEXEFTP", "PASSWORD", sChaîneCryptée, sFichierINI)
      INIEcrit("LANNEXEFTP", "PORT", SAI_port, sFichierINI)
      
      si fFichierExiste(sFichierINI) ALORS
      	info("Paramètres de configuration OK")
      	Ferme(FEN_Paramètres_de_connection_au_serveur_FTP)
      FIN
      
      
      
