      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      procédure UploadFTP()
      //Lecture du fichier INI avec les identifiants FTP
      sFichierINI est une chaine = fRepEnCours() + "\PARAM.INI"
      
      si INILit("LANNEXEFTP", "AUTH", "", sFichierINI) <> "" ALORS
      	info("LECTURE OK")
      	
      sinon
      	info("LECTURE PAS OK")
      	si	INIEcrit("LANNEXEFTP", "AUTH", "valeur de test", sFichierINI) ALORS
      		info("Ecriture OK")
      	sinon
      		Erreur(ErreurInfo())
      	FIN
      FIN
